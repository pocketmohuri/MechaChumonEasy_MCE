<%= form_with model:genre, url: url, local: true do |f| %>
	<%#= render 'layouts/error_messages', resource: genre %>
	<%= f.label :gemres_name, "ジャンル名:" %>
	<%= f.text_field :genres_name, style: "margin-right: 10px;" %>
	<%= f.radio_button :is_active, true %>
	<%= f.label :is_active, "有効", class: "text-active", style: "margin-right: 10px;" %>
	<%= f.radio_button :is_active, false %>
	<%= f.label :is_active, "無効", class: "text-invalid", style: "margin-right: 10px;" %>
	<%= f.submit submit, class: "btn btn-primary ml2" %>
<% end %>
